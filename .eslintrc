{
	"parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }	
    },
	"plugins": [
    	"react", "jest"	
  	],
	"rules": {
		"react/prop-types": 0,
		"no-console": 0
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"env": {
        "browser": true,
        "node": true,
		"es6":true,
		"jest/globals": true
    },
	"globals": {
        "ENV_NAME": true,
		"APP_NAME": true
    }
}
